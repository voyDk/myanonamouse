# Required
MAM_EMAIL=you@example.com
MAM_PASSWORD=your_password

# Optional behavior
MAM_BONUS_CAP=99999
MAM_BONUS_THRESHOLD=98000
MAM_BONUS_TARGET=90000
MAM_DONATE_POINTS=2000
MAM_MIN_UPLOAD_SPEND=500

# Optional runtime
MAM_APPLY=false
MAM_HEADLESS=true
MAM_TIMEOUT_MS=45000
MAM_LOGIN_URL=https://www.myanonamouse.net/login.php?returnto=%2Fstore.php
MAM_STORE_URL=https://www.myanonamouse.net/store.php
MAM_DEBUG_DIR=./debug

# Human-like pacing (recommended to keep true)
MAM_HUMANIZE=true
MAM_HUMAN_DELAY_MIN_MS=350
MAM_HUMAN_DELAY_MAX_MS=1300
MAM_TYPE_DELAY_MIN_MS=60
MAM_TYPE_DELAY_MAX_MS=140
MAM_PRE_CLICK_HOVER_MIN_MS=140
MAM_PRE_CLICK_HOVER_MAX_MS=420
MAM_SLOW_MO_MS=45

# Optional CLI defaults
MAM_CLI_DONATION_AMOUNT=2000
MAM_CLI_VIP_WEEKS=max
MAM_CLI_UPLOAD_GB=max

# Frontend runtime (React app)
VITE_APP_NAME=MyAnonamouse Control Room
VITE_BONUS_THRESHOLD=98000
VITE_BONUS_TARGET=90000
VITE_BONUS_CAP=99999
